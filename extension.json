{
  "name": "FlexForm",
  "version": "1.0.0 Beta 8",
  "author": [
    "[https://www.wikibase-solutions.com/author/charlot Sen-Sai]"
  ],
  "url": "https://www.mediawiki.org/wiki/Extension:FlexForm",
  "descriptionmsg": "flexform-desc",
  "license-name": "GPL v2",
  "type": "parserhook",
  "AutoloadClasses": {
    "ApiFlexForm": "ApiFlexForm.php",
    "FlexFormHooks": "FlexForm.hooks.php"
  },
  "AutoloadNamespaces": {
    "FlexForm\\": "src/"
  },
  "ExtensionMessagesFiles": {
    "WBFormAlias": "FlexForm.i18n.alias.php"
  },
  "APIModules": {
    "flexform": "ApiFlexForm"
  },
  "Hooks": {
    "ParserFirstCallInit": [
      "FlexFormHooks::onParserFirstCallInit"
    ],
    "AdminLinks": [
      "FlexFormHooks::addToAdminLinks"
    ]
  },
  "MessagesDirs": {
    "FlexForm": [
      "i18n"
    ]
  },
  "ServiceWiringFiles": [
    "FlexForm.ServiceWiring.php"
  ],
  "config": {
    "FlexFormConfig": {
      "file_temp_path": "",
      "rc_site_key": "",
      "rc_secret_key": "",
      "sec_key": "",
      "secure": false,
      "auto_save_interval": 30000,
      "auto_save_after_change": 3000,
      "use_smtp": false,
      "smtp_host": "",
      "smtp_authentication": true,
      "smtp_username": "",
      "smtp_password": "",
      "smtp_secure": "TLS",
      "smtp_port": 587,
      "use_form_builder": true,
      "allow_edit_docs": true,
      "form_timeout_limit": 7200,
      "allow_special_page_setup": true,
      "use_api_user_only": false,
      "debug": false,
      "FlexFormDefaultTheme": "Plain"
    }
  },
  "ResourceFileModulePaths": {
    "localBasePath": "",
    "remoteExtPath": "FlexForm"
  },
  "ResourceModules": {
    "ext.wsForm.slim.scripts": {
      "scripts": "Modules/slim/slim/slim.kickstart.min.js",
      "position": "bottom"
    },
    "ext.wsForm.ajax.scripts": {
      "scripts": "Modules/wsform-ajax.js",
      "position": "bottom"
    },
    "ext.wsForm.datePicker.scripts": {
      "scripts": "/simplegovscripts/bootstrap-datepicker.js",
      "position": "bottom"
    },
    "ext.wsForm.instance": {
      "scripts": "Modules/instances/wsInstance.js",
      "position": "top"
    },
    "ext.wsForm.scripts": {
      "scripts": "Modules/select2.js",
      "position": "top"
    },
    "ext.wsForm.styles": {
      "styles": "Modules/select2.css",
      "position": "top"
    },
    "ext.wsForm.general.styles": {
      "styles": "Modules/ext.WSForm.css",
      "position": "top"
    },
    "ext.FlexForm.Instance.styles": {
      "styles": "Modules/instances/instance-style.css",
      "position": "top"
    },
    "ext.wsForm.datePicker.styles": {
      "styles": "/simplegovscripts/bootstrap-datepicker.css",
      "position": "top"
    },
    "ext.wsForm.slim.styles": {
      "styles": "Modules/slim/slim/slim.min.css",
      "position": "top"
    }
  },
  "SpecialPages": {
    "FlexForm": "\\FlexForm\\Specials\\SpecialFlexForm"
  },
  "manifest_version": 1
}
