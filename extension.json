{
  "name": "WSForm",
  "version": "0.8.13",
  "author": [
    "[https://www.wikibase-solutions.com/author/charlot Sen-Sai]"
  ],
  "url": "https://bitbucket.org/wikibasesolutions/mw-wsform/",
  "descriptionmsg": "wsform-desc",
  "license-name": "GPL v2",
  "type": "parserhook",
  "AutoloadClasses": {
    "ApiWSForm": "ApiWSForm.php",
    "WSFormHooks": "WSForm.hooks.php"
  },
  "AutoloadNamespaces": {
    "WSForm\\": "src/"
  },
  "ExtensionMessagesFiles": {
    "WBFormAlias": "WSForm.i18n.alias.php"
  },
  "APIModules": {
    "wsform": "ApiWSForm"
  },
  "Hooks": {
    "ParserFirstCallInit": [
      "WSFormHooks::onParserFirstCallInit"
    ],
    "AdminLinks": [
      "WSFormHooks::addToAdminLinks"
    ]
  },
  "MessagesDirs": {
    "WSForm": [
      "i18n"
    ]
  },
  "ServiceWiringFiles": [
    "WSForm.ServiceWiring.php"
  ],
  "config": {
    "WSFormConfig": {
      "api_username": "",
      "api_password": "",
      "api_url_overrule": "",
      "api_cookie_path": "",
      "api_config_path": "",
      "file_temp_path": "",
      "wgCanonicalServer": "",
      "rc_site_key": "",
      "rc_secret_key": "",
      "sec_key": "",
      "auto_save_interval": 30000,
      "auto_save_after_change": 3000,
      "use_smtp": "no",
      "smtp_host": "",
      "smtp_authentication": true,
      "smtp_username": "",
      "smtp_password": "",
      "smtp_secure": "TLS",
      "smtp_port": 587,
      "use_form_builder": true,
      "allow_edit_docs": true,
      "form_timeout_limit": 7200,
      "allow_special_page_setup": true,
      "use_api_user_only": false,
      "WSFormDefaultTheme": "wsform"
    }
  },
  "ResourceFileModulePaths": {
    "localBasePath": "",
    "remoteExtPath": "WSForm"
  },
  "ResourceModules": {
    "ext.wsForm.slim.scripts": {
      "scripts": "modules/slim/slim/slim.kickstart.min.js",
      "position": "bottom"
    },
    "ext.wsForm.ajax.scripts": {
      "scripts": "wsform-ajax.js",
      "position": "bottom"
    },
    "ext.wsForm.datePicker.scripts": {
      "scripts": "/simplegovscripts/bootstrap-datepicker.js",
      "position": "bottom"
    },
    "ext.wsForm.instance": {
      "scripts": "modules/instances/wsInstance.js",
      "position": "top"
    },
    "ext.wsForm.scripts": {
      "scripts": "select2.js",
      "position": "top"
    },
    "ext.wsForm.styles": {
      "styles": "select2.css",
      "position": "top"
    },
    "ext.wsForm.general.styles": {
      "styles": "/modules/ext.WSForm.css",
      "position": "top"
    },
    "ext.wsForm.Instance.styles": {
      "styles": "modules/instances/instance-style.css",
      "position": "top"
    },
    "ext.wsForm.datePicker.styles": {
      "styles": "/simplegovscripts/bootstrap-datepicker.css",
      "position": "top"
    },
    "ext.wsForm.slim.styles": {
      "styles": "modules/slim/slim/slim.min.css",
      "position": "top"
    }
  },
  "SpecialPages": {
    "WSForm": "SpecialWSForm"
  },
  "manifest_version": 1
}
